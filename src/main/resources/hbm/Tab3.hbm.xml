<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.Tab3" table="tab3" catalog="chinavis2017" optimistic-lock="version">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>
        <property name="md5" type="string">
            <column name="md5" length="20"/>
        </property>
        <property name="content" type="string">
            <column name="content" length="25"/>
        </property>
        <property name="phone" type="string">
            <column name="phone" length="25"/>
        </property>
        <property name="connectime" type="string">
            <column name="connectime" length="25"/>
        </property>
        <property name="recitime" type="string">
            <column name="recitime" length="25"/>
        </property>
        <property name="lng" type="java.lang.Float">
            <column name="lng"/>
        </property>
        <property name="lat" type="java.lang.Float">
            <column name="lat"/>
        </property>
        <property name="type" type="java.lang.Integer">
            <column name="type"/>
        </property>
    </class>
</hibernate-mapping>